<%= simple_form_for(@season) do |f| %>

    <%= f.input :period, :collection => ["VT", "HT"] %><br />

    <%= f.input :year, :collection => 2011..2020 %><br />

    <div class="form-actions">
      <%= f.button :submit, :class => 'btn-primary' %>
      <%= link_to "Avbryt",
                seasons_path, :class => 'btn' %>
    </div>
<% end %>
